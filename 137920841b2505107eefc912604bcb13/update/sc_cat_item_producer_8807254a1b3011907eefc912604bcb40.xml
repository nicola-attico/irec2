        <script><![CDATA[try {
    var inputs = {};
    inputs['device_sys_id'] = producer.device.toString()
    inputs['registrant_sys_id'] = producer.device.toString().split("@")[1] + "@" + producer.device.toString().split("@")[2]
	inputs['policy_sys_id'] = producer.device.toString().split("@")[2]
	
	inputs['device_id'] = producer.device.toString().split("@")[0]; // String 
    inputs['registrant_id'] = producer.device.toString().split("@")[1]; // String 
    inputs['policy_id'] = producer.device.toString().split("@")[2]; // String 

	inputs['kwh'] = producer.kwh.toString(); // String 
    inputs['start_date'] = producer.start_date.toString(); // Date 
    inputs['end_date'] = producer.end_date.toString(); // Date 

    var result = sn_fd.FlowAPI.getRunner().action('x_snc_guardian.create_issuance_request').inForeground().withInputs(inputs).run();
    var outputs = result.getOutputs();

    var issuance = new GlideRecord("x_snc_guardian_st_issuance")
    issuance.addQuery("device", producer.device.toString())
    issuance.orderByDesc("seq")
    issuance.query()
    issuance.next()

    producer.redirect = issuance.getLink(false)
	producer.portal_redirect = "sp?id=form&table=x_snc_guardian_st_issuance&sys_id=" + issuance.sys_id

} catch (ex) {
    var message = ex.getMessage();
    gs.error(message);
}]]></script>
